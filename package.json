{
  "engines": {
    "node": ">=16.13"
  },
  "scripts": {
    "dev": "yarn intl && NODE_OPTIONS='--inspect' next",
    "build": "yarn intl && next build && yarn node ./post-build.js",
    "build:bundle": "ANALYZE=true next build",
    "intl:extract": "formatjs extract './**/*.messages.ts' --out-file app/lang/extracted/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "intl:compile": "formatjs compile-folder app/lang/extracted app/lang/compiled --ast",
    "intl": "yarn intl:extract && yarn intl:compile",
    "start": "next start",
    "lint": "eslint . && stylelint \"app/**/*.styles.ts\"",
    "lint:types": "tsc --noemit",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook --quiet",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@hookform/resolvers": "^2.8.5",
    "@next/bundle-analyzer": "^12.0.3",
    "@reduxjs/toolkit": "^1.5.0",
    "@sentry/nextjs": "^6.13.3",
    "axios": "^0.21.0",
    "change-case": "^4.1.1",
    "dotenv": "^8.2.0",
    "js-cookie": "^2.2.1",
    "jsondiffpatch": "^0.4.1",
    "jwt-decode": "^3.0.0",
    "next": "^12.0.3",
    "next-auth": "4.3.3",
    "next-redux-wrapper": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.1",
    "react-intl": "^5.24.0",
    "react-redux": "^7.2.2",
    "reduce-reducers": "^1.0.4",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.2",
    "styled-components": "^5.3.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@formatjs/cli": "^4.5.0",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@svgr/webpack": "^6.1.2",
    "@testing-library/dom": "^8.6.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/js-cookie": "^2.2.6",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash.assignin": "^4.2.6",
    "@types/node": "^16.11.15",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.9",
    "@types/styled-components": "^5.1.3",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "axios-mock-adapter": "^1.20.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-formatjs": "^10.3.12",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-destructuring-newline": "^0.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-formatjs": "^2.19.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-modules-newline": "^0.0.6",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.0",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "jest-dom": "^4.0.0",
    "jest-junit": "^13.0.0",
    "lint-staged": "^10.5.4",
    "lodash.assignin": "^4.2.0",
    "mockdate": "^3.0.5",
    "next-router-mock": "^0.6.3",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.0.2",
    "webpack": "5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:types && yarn lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn eslint",
      "yarn stylelint \"app/**/*.js\"",
      "yarn test --bail --findRelatedTests"
    ],
    "*.jsx": [
      "yarn eslint",
      "yarn stylelint \"app/**/*.jsx\"",
      "yarn test --bail --findRelatedTests"
    ],
    "*.ts": [
      "yarn eslint",
      "yarn stylelint \"app/**/*.ts\"",
      "yarn test --bail --findRelatedTests"
    ],
    "*.tsx": [
      "yarn eslint",
      "yarn stylelint \"app/**/*.tsx\"",
      "yarn test --bail --findRelatedTests"
    ]
  }
}
